// Peak Usage Hours
// Bot usage by hour of day (UTC)
dependencies
| where timestamp > ago(7d)
| where name == "Custom Event"
| where customDimensions["event.name"] == "Message_Received"
| extend HourOfDay = hourofday(timestamp)
| summarize MessageCount = count() by HourOfDay
| render columnchart title="Bot Usage by Hour (UTC)"
